{
  "name": "logger-api",
  "version": "1.0.0",
  "description": "An app for logging hours on projects using express & mongoose.",
  "engines": {
    "node": ">=16"
  },
  "main": "index.js",
  "type": "commonjs",
  "scripts": {
    "start": "node --experimental-modules --es-module-specifier-resolution=node src/index.js",
    "test": "jest --config jest.config.cjs --coverage=false",
    "prepare": "husky install",
    "dev": "nodemon --experimental-modules --es-module-specifier-resolution=node src/index.js",
    "lint": "eslint \"src/**/*.js\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/inayatullahaaxioned/NodeJsTest"
  },
  "author": "inayatullaha@axioned.com",
  "license": "ISC",
  "devDependencies": {
    "husky": "^8.0.3",
    "jest": "^29.5.0",
    "nodemon": "^2.0.22"
  },
  "eslintIgnore": [
    "node_modules",
    "src/tests"
  ],
  "dependencies": {
    "bcrypt": "^5.1.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.9.1",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^7.0.3"
  }
}
